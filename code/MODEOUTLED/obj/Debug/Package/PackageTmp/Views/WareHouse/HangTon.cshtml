        <th class="Number">Tổng lượng tồn</th>
        <th class="Function">Chức năng</th>
    </tr>
    @Html.Raw(ViewBag.View)
</table>
<div class="Control">
    <ul>
        <li>@Html.ActionLink("Làm mới", "HangTon", "", new { @class = "vrefresh" })</li>
