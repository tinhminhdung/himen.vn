@{
    ViewBag.Title = "FromdaytoDay";
    Layout = "~/Views/Shared/_LayoutAdmins.cshtml";
}
<script type="text/javascript" src="http://www.google.com/jsapi"></script>
@Html.Raw(ViewBag.script)
<script src="~/Scripts/jquery-1.9.1.js"></script>
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/jquery.datepick.js"></script>
<script type="text/javascript">
    $(function () {
        $('#txtDate,#txtDateto').focus(function () {
            $(this).val('');
        });
    });
</script>
<script type="text/javascript">

    $(function () {
        $("#txtDate").datepicker();
        $("#txtDateto").datepicker();
    })
</script>

@model IList<onsoft.Models.sp_GroupProduct_Baocao_Result>

<div style="margin:0 auto; width:700px">
@using(Html.BeginForm())
{
    if (Model != null) {
    <input type="text" name="txtDate" id="txtDate" value="@ViewBag.Date" />
    <input type="text" name="txtDateto" id="txtDateto" value="@ViewBag.Dateto" />
    } else { 
    @Html.TextBox("txtDate");
    @Html.TextBox("txtDateto");
}
    <input type="submit" value="nhap" />
    <div>
        <table class="table table-striped table-bordered dataTable table-hover" cellspacing="0" id="cph_Main_ctl00_ctl00_grvProducts" style="border-collapse: collapse; text-align:center">
            <tr style="text-align:center">
                <th style="text-align:center;">Group ID</th>
                <th style="text-align:center;">Group Name</th>
                <th style="text-align:center;"> Number</th>
            </tr>
            @if (Model != null)
            {
                for (int i = 0; i < Model.Count; i++)
                {
                    <tr style="text-align:center">
                        <td style="text-align:center;">@Model[i].GroupID</td>
                        <td style="text-align:center;">@Model[i].GroupName</td>
                        <td style="text-align:center;">@Model[i].Soluongmua</td>
                    </tr>
                }
            }
        </table>

    </div>
    
}
    <div id="chart" style="width:100%"> </div>
</div>


