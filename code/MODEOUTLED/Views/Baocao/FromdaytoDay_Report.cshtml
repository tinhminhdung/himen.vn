@{
    ViewBag.Title = "FromdaytoDay_Report";
    Layout = "~/Views/Shared/_LayoutAdmins.cshtml";
}



@model IList<onsoft.Models.sp_GroupProduct_Baocao_Result>
<h2>FromdaytoDay_Report</h2>
<table >
    <tr>
        <th>Group ID</th>
        <th>Group Name</th>
        <th> Number</th>
    </tr>
    @for(int i=0; i<Model.Count;i++)
    { 
    <tr>
     <td>@Model[i].GroupID</td>
        <td>@Model[i].GroupName</td>
        <td>@Model[i].Soluongmua</td>   
    </tr>
    }
</table>