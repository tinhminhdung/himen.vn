.logintop      { width: 155px; background: url(images/bg-cart.png); height: 28px; border: 1px solid #dddddd; border-radius: 2px;  
                 text-align: center; display: table; float: left; margin: 18px 5px 0 25px }
.logintop span { line-height: 28px; color:#333; font-weight: normal; font-size: 12px; }
.logintop p    { line-height: 20px; font-weight: normal; font-size: 12px; color: #dddddd; display: inline; padding: 0 5px}
.logintop span:hover { font-weight: bold}
.ViewCart                               { width: 116px; background:  url(images/bg-cart.png); height: 28px; float: left;
                                          border: 1px solid #dddddd; border-radius: 2px; }
.ViewCart a:first-child                 { background: url(images/icon-cart.png) no-repeat 10px 0; width: 116px; height: 28px; padding: 0 7px; display:block;}
.ViewCart span.numCart                  { color:#f00; font-weight: bold; font-size: 12px;
                                           width: 90px; text-align: center; padding-left: 85px}
.ViewCart .showCartTop                  { display: none; background: #005af5; border-top: 2px solid #be071b; width: 200px; left: 0;  border-radius: 0 5px 5px 0; box-shadow:0px 0px 3px #333333; padding: 3px; }
.ViewCart .showCartTop ul li { list-style: none; clear: both}
.ViewCart .showCartTop ul li a { color: #fff; text-decoration: none}
.showCartTop img                        { max-height:30px; max-width:30px; float:left; margin:5px 5px 0 0;}

.ViewCart:hover { box-shadow: 2px 2px 2px #eaeaea; border-radius: 2px;}
.logintop:hover { box-shadow: 2px 2px 2px #eaeaea; border-radius: 2px;}
.ViewCart:hover .numCart,.ViewCart a.activeCart .numCart {}
.ViewCart .showCartTop p                { color: #fff; display: table; background:#005af5; width:200px; left:0px;}
.proCart                                { margin:3px;}
.proCart a                              { color:#fff}
.proCart a:hover { color: #fff}
.proCart span                           { color:#783235; font-weight:bold;}
.pCartView                              { width: 200px; bottom:2px; margin-top:5px}
.pCartView p { clear: left}
.pCartView a.btn-view-cart              { display:block;background:#fff; background:-moz-linear-gradient(top, #bcbbbb 0%, #bcbbbb 100%); background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#bcbbbb), color-stop(100%,#bcbbbb)); background:-webkit-linear-gradient(top, #bcbbbb 0%,#bcbbbb 100%); background:-o-linear-gradient(top, #f93f3f 0%,#eb2a2a 100%); background:-ms-linear-gradient(top, #bcbbbb 0%,#bcbbbb 100%); background:linear-gradient(to bottom, #bcbbbb 0%,#bcbbbb 100%); filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#bcbbbb', endColorstr='#bcbbbb',GradientType=0 ); line-height:25px; height:25px; padding:0; text-align:center; border-radius:4px; color:#fff; width:100%; margin: 0}


